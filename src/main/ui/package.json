{
  "name": "discord-tunnel",
  "proxy": "http://localhost:8080",
  "author": "Florian Trayon (https://github.com/FlorianLeChat)",
  "license": "MIT",
  "private": true,
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlorianLeChat/Discord-Tunnel.git"
  },
  "bugs": {
    "url": "https://github.com/FlorianLeChat/Discord-Tunnel/issues"
  },
  "dependencies": {
    "next": "~13.4",
    "postcss": "~8.4",
    "tailwindcss": "~3.3",
    "autoprefixer": "~10.4"
  },
  "devDependencies": {
    "@types/node": "~20.2",
    "@types/react": "~18.2",
    "pm2": "~5.3",
    "typescript": "~5.1",
    "eslint-config-next": "~13.4",
    "eslint-config-airbnb": "~19.0",
    "@typescript-eslint/parser": "~5.60",
    "@total-typescript/ts-reset": "~0.4",
    "@typescript-eslint/eslint-plugin": "~5.59"
  },
  "engines": {
    "node": ">=18.0",
    "npm": ">=8.6"
  },
  "overrides": {
    "uuid": "~9.0"
  },
  "scripts": {
    "dev": "next dev",
    "lint": "eslint . --config .eslintrc.json --ext .js,.jsx,.ts,.tsx",
    "build": "next build && cd ../../.. && ./mvnw clean install -DskipTests",
    "start": "pm2 start --name discord-tunnel java -- -jar ../../../target/discord-tunnel-1.0.0.jar",
    "stop": "pm2 delete discord-tunnel",
    "restart": "pm2 restart discord-tunnel --update-env",
    "reload": "pm2 reload discord-tunnel"
  },
  "browserslist": [
    "defaults"
  ]
}
